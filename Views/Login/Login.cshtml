@model AdminMVC.Models.PersonalModel
<div class="centerbody">
    <h2>Logga in</h2>

    @using (Html.BeginForm())
    {
            
            <div>Användarnamn</div>
            <div>@Html.EditorFor(m => m.Email) </div>
            <div>Lösenord</div>
            <div>@Html.PasswordFor(m => m.Losenord)</div>
            <br />
            <input type="submit" value="Logga in" />
            <div>
                @Html.ValidationSummary()
            </div>

    }
</div>
